@model IEnumerable<SGCFIEE.Models.Calificaciones_Alumno_Movilidad>;
@{
    Layout = "~/Views/Home/Principal.cshtml";
    var x = (String)ViewData["consejo"];
}
<!DOCTYPE html>
<html>
<div class="profile-content">
    <div class="row">
        <div class="col-md-12 col-sm-12">
            <div class="card">
                <div class="card-topline-green"></div>
                <div class="white-box">
                    <div class="p-rl-20">
                        <ul class="nav customtab nav-tabs" role="tablist">
                            <li class="nav-item"><a href="#tab1" class="nav-link active show" data-toggle="tab">Parametros evaluados</a></li>
                        </ul>
                    </div>
                    <div class="tab-content">
                        <div class="tab-pane fontawesome-demo active show" id="tab1">
                            <div class="row">
                                <div class="col-md-12 col-sm-12">

                                    <div class="card-body " id="bar-parent2">
                                        <div class="row">
                                            <table class="table table-striped table-bordered table-hover table-checkable order-column" style="width: 100%" id="example1">
                                                <thead>
                                                    <tr>
                                                        <th class="center">Nombre de la materia</th>
                                                        <th class="center">Calificacion</th>
                                                        <th class="center">Creditos</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @foreach (var item in Model)
                                                    {
                                                        <tr class="odd gradeX">
                                                            <td class="center">@Html.DisplayFor(model => item.NombreMateria)</td>
                                                            <td class="center">@Html.DisplayFor(model => item.calificacion)</td>
                                                            <td class="center">@Html.DisplayFor(model => item.creditos)</td>
                                                        </tr>
                                                    }

                                                </tbody>
                                            </table>
                                            <div class="col-md-6 col-6 b-r">
                                                <strong>@Html.DisplayFor(model => x)</strong>
                                                <br />
                                            </div>
                                            <a id="cancelarEdicion" asp-controller="EstudiantesMovilidad" asp-action="Index" class="btn btn-default btn-sm m-b-10">
                                                Volver
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</html>