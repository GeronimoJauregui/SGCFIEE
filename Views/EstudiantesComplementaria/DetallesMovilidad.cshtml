@model SGCFIEE.Models.CtMovilidades
@{
    ViewData["Title"] = "Dtalle Movilidad";
    Layout = "~/Views/Home/Principal.cshtml";
}

<!DOCTYPE html>

<html>
<div class="profile-content">
    <div class="row">
        <div class="col-md-12 col-sm-12">
            <div class="card">
                <div class="card-topline-green"></div>

                <div class="p-rl-20">
                    <ul class="nav customtab nav-tabs" role="tablist">
                        <li id="tb1" class="nav-item"><a href="#tab1" class="nav-link active show" data-toggle="tab">Información De Movilidad</a></li>
                    </ul>
                </div>
                <!-- Tab panes -->
                <div class="tab-content">
                    <div class="tab-pane fontawesome-demo active show" id="tab1">
                        <div class="row">
                            <div class="col-md-12 col-sm-12">

                                <div class="card-body " id="bar-parent2">
                                    <div class="row">
                                        <div class="col-md-6 col-6 b-r">
                                            <strong>Tipo De Movilidad</strong>
                                            <br>
                                            <p id="" class="text-muted">@Html.DisplayFor(model => model.TipoMovilidades)</p>
                                            <br>
                                        </div>
                                        <div class="col-md-6 col-6 b-r">
                                            <strong>Pais Destino</strong>
                                            <br>
                                            <p id="" class="text-muted">@Html.DisplayFor(model => model.PaisDestinoMovilidad)</p>
                                            <br>
                                        </div>
                                        <div class="col-md-6 col-6 b-r">
                                            <strong>Entidad Destino</strong>
                                            <br>
                                            <p id="" class="text-muted">@Html.DisplayFor(model => model.EntidadDestinoMovilidad)</p>
                                            <br>
                                        </div>
                                        <div class="col-md-6 col-6">
                                            <strong>Escuela Destino</strong>
                                            <br>
                                            <p id="" class="text-muted">@Html.DisplayFor(model => model.EscuelaDestinoMovilidad)</p>
                                            <br>
                                        </div>
                                        <div class="col-md-6 col-6">
                                            <strong>Tiempo De Permanencia</strong>
                                            <br>
                                            <p id="" class="text-muted">@Html.DisplayFor(model => model.TiempoPermanenciaMovilidad)</p>
                                            <br>
                                        </div>
                                    </div>
                                    <a asp-controller="EstudiantesComplementaria" asp-action="MostrarMovilidad" class="btn btn-success btn-sm m-b-10">
                                        Regresar a tabla
                                    </a>
                                    <a asp-controller="EstudiantesComplementaria" asp-action="Index" class="btn btn-success btn-sm m-b-10">
                                        Regresar a principal
                                    </a>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</html>
